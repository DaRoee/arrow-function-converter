{
    "name": "arrow-function-converter",
    "displayName": "Arrow Function Converter",
    "description": "",
    "version": "0.1.1",
    "publisher": "Roee",
    "engines": {
        "vscode": "^1.26.0"
    },
    "bugs": {
        "url": "https://github.com/DaRoee/arrow-function-converter/issues"
    },
    "homepage": "https://github.com/DaRoee/arrow-function-converter/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/DaRoee/arrow-function-converter/"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.arrowFunctionConverter"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.arrowFunctionConverter",
                "title": "Convert 1 liner method to multiline"
            }
        ],
        "keybindings": [
            {
                "command": "extension.arrowFunctionConverter",
                "key": "alt+shift+k",
                "mac": "alt+shift+k",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.39",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    }
}
